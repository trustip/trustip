{
    "name": "trustip/trustip",
    "description": "A Laravel package from Trustip.io for VPN and Proxy detection, it allows you to check if an IP address is a Proxy, VPN, TOR, Hosting, Bad search engine robots or not.",
    "version": "1.0",
    "license": "MIT",
    "authors": [{
        "name": "Trustip.io",
        "email": "support@trustip.io"
    }],
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Trustip\\Trustip\\": "src/"
        },
        "files": [
            "src/Helpers/ProxyCheckHelper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trustip\\Trustip\\TrustipServiceProvider"
            ],
            "aliases": {
                "Trustip": "Trustip\\Trustip\\Facades\\Trustip"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
